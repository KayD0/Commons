<script type="text/javascript">
    $( function() {
        $(document).on('click', '#open-new-tab-download-link', function () {
            OpenNewTab('@Url.Action("DownloadOpenNewTab","Home")');
            //$('#open-new-tab-download').submit();
        });
    });

    function OpenNewTab(url)
    {
        window.open(url, "_blank");
    }
</script>