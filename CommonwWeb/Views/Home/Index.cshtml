@model CommonwWeb.Models.HomeViewModel
@{
    ViewData["Title"] = "Home Page";
}


<!-- 以下を参考にしてjsのみで非同期通信を行う -->
<!-- https://qiita.com/kenta8813/items/d0307c258d61ecebb064 -->
    <div>
        <!-- ダウンロード基本 -->
        <form id="download" action="@Url.Action("Download","Home")" method="post">
            @Html.AntiForgeryToken()
            <input type="hidden" class="download-item" name="iid" value="2">
            <input type="submit" id="btn-basic-download" value="btn-download">
        </form>
        <form id="open-download" action="@Url.Action("DownloadOpen","Home")" method="post">
            @Html.AntiForgeryToken()
            <input type="hidden" class="open-download-item" name="iid" value="2">
            <input type="submit" id="btn-open-download" value="btn-open-download">
        </form>
        <form target="_blank" id="open-new-tab-download" action="@Url.Action("DownloadOpenNewTab","Home")" method="post">
            @Html.AntiForgeryToken()
            <input type="hidden" class="open-new-tab-download-item" name="iid" value="2">
            <input type="submit" id="btn-open-new-tab-download" value="btn-open-new-tab-download">
        </form>
    </div>